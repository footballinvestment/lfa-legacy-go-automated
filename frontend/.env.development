# AGGRESSIVE memory optimization for development
GENERATE_SOURCEMAP=false
DISABLE_ESLINT_PLUGIN=true
FAST_REFRESH=false
INLINE_RUNTIME_CHUNK=false
WDS_SOCKET_PORT=0

# API Configuration
REACT_APP_API_URL=http://localhost:8000

# Disable ALL unnecessary features for memory savings
BROWSER=none
TSC_COMPILE_ON_ERROR=true
SKIP_PREFLIGHT_CHECK=true

# TypeScript optimization
TYPESCRIPT_DISABLE_SERVICE=true
TS_NODE_COMPILER_OPTIONS={"module":"commonjs","target":"es2015"}

# Webpack dev server optimization
CHOKIDAR_USEPOLLING=false
WATCHPACK_POLLING=false

# React optimization flags
REACT_APP_ENABLE_MEMORY_OPTIMIZATION=true

# Disable hot reload for memory savings
HOT=false